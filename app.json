{
  "expo": {
    "name": "EbdeiTypeScript",
    "slug": "EbdeiTypeScript",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seudominio.ebdeitypescript"
    },
    "android": {
      "package": "com.seudominio.ebdeitypescript",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos.",
          "cameraPermission": "O aplicativo precisa acessar a c√¢mera."
        }
      ]
    ]
  }
}
