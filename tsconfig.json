{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true, 
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "moduleResolution": "node", 
    "module": "ESNext",          
    "target": "ESNext" 
  },
  "extends": "expo/tsconfig.base"
}
